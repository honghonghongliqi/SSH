<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.study.entity.Admin" table="admin" schema="book">
        <id name="id">
            <column name="id" sql-type="varchar(11)" length="11"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>