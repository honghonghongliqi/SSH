<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.study.entity.Record" table="record" schema="book">
        <id name="id">
            <column name="id" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="num">
            <column name="num" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="people">
            <column name="people" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="bookId">
            <column name="bookId" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>