<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.study.entity.Book" table="book" schema="book">
        <id name="id">
            <column name="id" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="picture">
            <column name="picture" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="author">
            <column name="author" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="number">
            <column name="number" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="place">
            <column name="place" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>