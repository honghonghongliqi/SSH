<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.study.entity.People" table="people" schema="book">
        <id name="id">
            <column name="id" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="sex">
            <column name="sex" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>

        <property name="password">
            <column name="password" sql-type="varchar(30)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>